{{#each this}}
<li class="card">
  <h2>
    {{name}}
  </h2>
  <div class="about">
    <div class="info">
    <p><span>Capital:</span>{{capital}}</p>
  <p>
    <span>Population:</span>{{population}}
  </p>
  <ul class="languagesList"><span>Languages:</span> {{#each languages}}
    <li class='languagesItem'>
      {{this.name}}
    </li>
    {{/each}}
  </ul>
     </div>
     <img src="{{flag}}" alt="{{name}}" width="460">
   </div>
  </li>
{{/each}}